        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "for planet in array:    #for文\n",
        "    print(planet)\n",
        "\n",
        "i = 0\n",
        "while i < len(array):   #while文\n",
        "    print(array[i])\n",
        "    i = i + 1\n",
        "    if i == len(array):\n",
        "        break"
