        "import random\n",
        "\n",
        "var = random.randint(0, 100)\n",
        "#print(var)\n",
        "\n",
        "if var % 3 == 0 and var % 5 == 0:   #3の倍数と5の倍数の両方\n",
        "    print(\"FizzBuzz\")\n",
        "elif var % 3 == 0 :                 #3の倍数\n",
        "    print(\"Fizz\")\n",
        "elif var % 5 == 0 :                 #5の倍数\n",
        "    print(\"Buzz\")\n",
        "else:\n",
        "    print(var)"
